<odoo>

    <record model="ir.ui.view" id="account_financial_report_id_form_view">
        <field name="name">account.financial.report.form (in account_report_id)</field>
        <field name="model">account.move.line</field>
        <field name="inherit_id" ref="account.view_move_line_form"/>
        <field name="arch" type="xml">
            <field name="journal_id" position="after">
                <field name="account_report_id"/>
            </field>
        </field>
    </record>


    <record model="ir.ui.view" id="account_financial_report_id_tree_view">
        <field name="name">account.financial.report.tree (in account_report_id)</field>
        <field name="model">account.move.line</field>
        <field name="inherit_id" ref="account.view_move_line_tree"/>
        <field name="arch" type="xml">
            <field name="journal_id" position="after">
                <field name="account_report_id"/>
            </field>
        </field>
    </record>

</odoo>
